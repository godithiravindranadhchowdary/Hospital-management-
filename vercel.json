{
  "builds": [
    {
      "src": "config/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.11" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "config/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SECRET_KEY": "@django-insecure-hospital-key-2024",
    "DJANGO_DEBUG": "False",
    "DATABASE_ENGINE": "django.db.backends.sqlite3"
  }
}
